<view class="page">
  <view class="page__hd">
    <text class="page__title">map</text>
  </view>
  <view class="page__bd">
    <view class="section section_gap">
      <map
        style="width: 100%; height: 300px;"
        latitude="{{latitude}}"
        longitude="{{longitude}}"
        markers="{{markers}}"
        covers="{{covers}}"
        >
      </map>
    </view>
  </view>
</view>
